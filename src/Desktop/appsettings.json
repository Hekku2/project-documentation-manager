{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Desktop": "Debug"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "HH:mm:ss "
      }
    }
  },
  "ApplicationOptions": {
    "DefaultProjectFolder": "./example-projects/basic-features",
    "DefaultTheme": "Dark"
  }
}